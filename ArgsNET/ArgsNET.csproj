<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>ArgsNET</Title>
    <Authors>Oscar_Marcusson</Authors>
		<Version>1.0.1</Version>
    <Description>A .NET command line argument parser. It can be used without any configuration, as long as you have a class that can receive the data. 

Get started by calling `ArgsNET.Deserialize`</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/OscarMarcusson/ArgsNET</RepositoryUrl>
    <PackageTags>cli,arguments,parser,deserialize,fluent</PackageTags>
    <PackageIcon>ArgsNET-package-icon.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Icons\ArgsNET-package-icon.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
